{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "routes": [
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/css/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/cursos/(.*)",
            "dest": "/cursos/$1"
        },
        {
            "src": "/funcionario/(.*)",
            "dest": "/funcionario/$1"
        },
        {
            "src": "/educador/(.*)",
            "dest": "/educador/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "headers": [
        {
            "source": "/js/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript; charset=utf-8"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/css/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/css; charset=utf-8"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}